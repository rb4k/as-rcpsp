<h1>Vorgang</h1>

<p>
  <b>Name:</b>
  <%= @procedure.name %>
</p>

<p>
  <b>Projektzugehörigkeit:</b>
<td><%= project = Project.find(@procedure.project_id)
        project.name %></td>
</p>

<p>
  <b>Kapazitätsbedarf:</b>
  <%= @procedure.kapabe %>
</p>

<p>
  <b>Vorgangsdauer:</b>
  <%= @procedure.prot %>
</p>


<%= button_to 'Ändern', edit_procedure_path(@procedure), :class => "button", :method => :get %>
<%= button_to 'Zurück', procedures_path, :class => "button", :method => :get %>
